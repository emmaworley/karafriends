  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.(ts|tsx)": "tslint --fix",
    "**/*.rs": "rustfmt"
  },
  "packageManager": "yarn@4.6.0",
  "targets": {
    "main_": {
      "source": "src/main/index.ts",
      "context": "electron-main",
      "includeNodeModules": true
    },
    "preload": {
      "source": "src/preload/index.ts",
      "context": "electron-renderer",
      "includeNodeModules": true,
      "scopeHoist": true
    },
    "renderer": {
      "source": "src/renderer/index.html",
      "context": "browser"
    },
    "remocon": {
      "source": "src/remocon/index.html",
      "context": "browser"
    }
  },
  "@parcel/bundler-default": {
    "minBundles": 100
  },
  "relay": {
    "src": "src",
    "schema": "src/common/schema.graphql",
    "language": "typescript"
  },
  "alias": {
    "@parcel/error-overlay": "./src/common/disableParcelErrorOverlay.ts",
    "canvas": false
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "dependenciesMeta": {
    "@electron/osx-sign@1.3.2": {
      "unplugged": true
    }
  }
}
